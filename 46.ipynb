    "def fact(num):\n",
    "    if num>0:\n",
    "        return (num*fact(num-1))\n",
    "    else:\n",
    "        return 1\n",
    "\n",
    "def findZeroesInNumber():\n",
    "    k = fact(155)\n",
    "    sum = 0\n",
    "    for i in str(k):\n",
    "        if i == '0':\n",
    "            sum = sum +1"
