    "def findSeries(l):\n",
    "    diff = (l[0] + l[len(l)-1])//len(l)\n",
    "    print(diff)\n",
    "    for i in range(1,len(l)):\n",
    "        if abs(l[i] - l[i-1]) != diff:\n",
    "            print(\"They are not in series\")\n",
    "            return 0\n",
    "    l.append(l[len(l)-1]+diff)\n",
    "    return l\n",
    "\n",
    "print(findSeries([1,3,5]))"
   ]
